{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "outputs": ["dist/**"]
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  },
  "globalDependencies": ["tsconfig.json", "tsup.config.ts", "typescript"]
}
